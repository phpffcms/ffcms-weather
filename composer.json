{
  "name": "phpffcms/ffcms-weather",
  "description": "Weather application for ffcms 3 to use openweathermap.org api data and mapbox.com maps",
  "type": "library",
  "keywords": ["php", "ffcms", "app", "weather", "api", "map"],
  "homepage": "http://ffcms.org",
  "license": "MIT",
  "authors": [
    {
      "name": "Pyatinskyi M.M.",
      "email": "zenn1989@gmail.com",
      "homepage": "http://ffcms.ru"
    }
  ],
  "require": {
    "php": ">=5.5",
    "bower-asset/leaflet": "1.0.*@stable"
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
